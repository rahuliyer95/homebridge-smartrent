{
  "displayName": "SmartRent",
  "name": "@rahuliyer95/homebridge-smartrent",
  "version": "1.3.0",
  "description": "Homebridge plugin for SmartRent installations",
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "hap",
    "homekit",
    "smartrent"
  ],
  "license": "Apache-2.0",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "tag": "latest",
    "access": "public"
  },
  "author": "Rahul Iyer",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rahuliyer95/homebridge-smartrent.git"
  },
  "engines": {
    "node": "^20.16.0 || ^22.5.1 || ^24.0.0",
    "homebridge": "^1.8.0 || ^2.0.0-beta.0"
  },
  "main": "dist/index.js",
  "files": [
    "src",
    "homebridge-ui",
    "dist",
    "config.example.json",
    "config.schema.json",
    "CHANGELOG.md",
    "screenshot.png"
  ],
  "scripts": {
    "prepare": "husky",
    "typecheck": "tsc --noEmit",
    "prettier": "prettier --check .",
    "format": "prettier --write .",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "clean": "rimraf ./dist",
    "compile": "tsc",
    "build": "npm-run-all clean compile",
    "watch": "npm run build && npm link && nodemon",
    "prepublishOnly": "npm-run-all prettier lint build",
    "semantic-release": "cross-env semantic-release"
  },
  "dependencies": {
    "@homebridge/plugin-ui-utils": "^2.0.0",
    "axios": "^1.7.7",
    "axios-retry": "^4.5.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.0.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.1.1",
    "@semantic-release/release-notes-generator": "^14.0.1",
    "@types/node": "^25.0.3",
    "@types/ws": "^8.5.12",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "conventional-changelog-conventionalcommits": "^9.0.0",
    "cross-env": "^10.0.0",
    "eslint": "^9.12.0",
    "homebridge": "^1.4.0",
    "husky": "^9.1.6",
    "lint-staged": "^16.0.0",
    "nodemon": "^3.1.7",
    "npm-run-all": "^1.1.3",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "semantic-release": "^25.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^5.6.3"
  },
  "types": "./dist/index.d.ts",
  "bugs": {
    "url": "https://github.com/rahuliyer95/homebridge-smartrent/issues"
  },
  "homepage": "https://github.com/rahuliyer95/homebridge-smartrent#readme"
}
